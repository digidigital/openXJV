         <xs:restriction base="xoev-code:Code">
            <xs:sequence>
               <xs:element name="code" type="xs:token"/>
            </xs:sequence>
            <xs:attribute name="listURI" type="xs:anyURI" use="optional"
               fixed="urn:xoev-de:kosit:codeliste:country-codes"/>
