                                    <xs:sequence>
                                       <xs:element name="loeschung" maxOccurs="unbounded">
                                          <xs:complexType>
                                             <xs:sequence>
                                                <xs:element name="veroeffentlichung.id" type="xoev-lc:String.Latin"/>
