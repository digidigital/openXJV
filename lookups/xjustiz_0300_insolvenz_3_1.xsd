                                                <xs:element name="verarbeitungszeitpunkt" type="xs:dateTime"/>
                                                <xs:element name="verarbeitungsergebnis"
                                                            type="tns:Code.INSO.Ergebnis.VOE_Verarbeitungsbestaetigung"/>
